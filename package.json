{
  "name": "hotdoc",
  "version": "0.6.0",
  "description": "A SASSier responsive documentation template theme for JSDoc 3 based on docdash",
  "main": "template/publish.js",
  "scripts": {
    "test": "jsdoc -c fixtures/fixtures.conf.json",
    "sync": "browser-sync start -s ../fixtures-doc -f ../fixtures-doc --reload-delay 1000 --no-ui --no-notify",
    "watch": "watch-run -d 1000 -p tmpl/**,static/** \"npm run test\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kmck/hotdoc.git"
  },
  "dependencies": {
    "autoprefixer": "^6.3.7",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "lodash": "^4.13.1",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.9.1",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "text-transform-loader": "^1.1.1",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "jsdoc": "latest",
    "browser-sync": "latest",
    "watch-run": "latest"
  },
  "author": "Keith McKnight <keith@mcknig.ht>",
  "license": "Apache-2.0",
  "keywords": [
    "jsdoc",
    "template"
  ]
}
